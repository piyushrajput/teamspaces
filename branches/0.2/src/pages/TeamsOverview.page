			<c:TeamsWidgetMyTasksAndMilestones />  
			
			<!-- Popular Discussions -->
			<c:TeamsWidgetPopularDiscussions />  
			
			<!-- Activity Feed -->	
			<c:TeamsWidgetActivityFeed />  
			
			
			<!-- Recently Created Teams 
			<c:TeamsRecentlyCreated />  
			 
			<!-- 
			<c:RecentTeamsMemberJoined /> 	-->					
		</div>		

	</div>

	<script>
		/* scripts to reset the hieght of the lower bar so that it show correctly at the bottom of it all */
		// setTimeout('redim();', 2000);
		window.onload = footerBug; 
		function footerBug(){
			setTimeout('footer();', 500);
			
		}
		
		function footer(){
			var sa = $$('.rightCol')[0].getHeight();
			var sal = $('holderNew').getHeight();
			if(sa > sal){
			    $('holderNew').style.height = sa+'px';
			}
			else {
				$('holderNew').style.height = 'auto';
			}
		}
		
	</script>
